package mypage;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;

import javax.swing.JTextField;

public class Member implements ActionListener{
	private JTextField id,pw,name,tel,gender;

	
	//회원정보 저장
	public Member(JTextField id, JTextField pw, JTextField name, JTextField tel, JTextField gender) {
		this.id = id;
		this.pw = pw;
		this.name = name;
		this.tel = tel;
		this.gender = gender;
	}
	
	//회원가입 버튼 눌렀을 때 저장할 정보를 
	@Override
	public void actionPerformed(ActionEvent e) {
		try {
			FileOutputStream fos = new FileOutputStream("D:\\java_study\\member.txt",true);
		} catch (Exception e1) {
			
			e1.printStackTrace();
		}
		
	}
	
	//회원정보 조회
	public void MemberCheck() {
		
	}
}
